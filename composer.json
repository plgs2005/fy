{
    "name": "wmbonfiglio/influencer-project",
    "type": "project",
    "description": "Influencer Project - Nome a definir",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "beyondcode/laravel-websockets": "^1.12",
        "doctrine/dbal": "2.12.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^7.24",
        "laravel/sanctum": "^2.8",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.4.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "pusher/pusher-php-server": "~4.0",
        "riverskies/laravel-mobile-detect": "^1.3",
        "roerjo/laravel-notifications-sendgrid-driver": "^1.3",
        "s-ichikawa/laravel-sendgrid-driver": "^3.0",
        "sendgrid/sendgrid": "~7",
        "spatie/laravel-permission": "^3.18",
        "stripe/stripe-php": "^7.67"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "jeroenzwart/laravel-csv-seeder": "^1.5",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
